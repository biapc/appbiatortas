<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CardapioActivity">

    <RelativeLayout
        android:id="@+id/relative_layout"
        android:layout_width="384dp"
        android:layout_height="592dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.506">

        <ImageView
            android:id="@+id/imageView_bkg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            app:srcCompat="@drawable/background" />

        <ImageView
            android:id="@+id/imageView_logo"
            android:layout_width="226dp"
            android:layout_height="190dp"
            android:layout_alignStart="@+id/imageView_bkg"
            android:layout_alignLeft="@+id/imageView_bkg"
            android:layout_alignTop="@+id/imageView_bkg"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="77dp"
            android:layout_marginLeft="77dp"
            android:layout_marginTop="35dp"
            android:layout_marginEnd="81dp"
            android:layout_marginRight="81dp"
            app:srcCompat="@drawable/logo" />

        <ImageView
            android:id="@+id/imageView_background"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:contentDescription="asffdfsdfsdfsfd"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/background" />

        <ImageButton
            android:id="@+id/imageButton_torta1"
            android:layout_width="112dp"
            android:layout_height="102dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="57dp"
            android:layout_marginLeft="57dp"
            android:layout_marginBottom="242dp"
            app:srcCompat="@mipmap/ic_launcher"
            tools:srcCompat="@drawable/alema" />

        <ImageButton
            android:id="@+id/imageButton_torta2"
            android:layout_width="116dp"
            android:layout_height="96dp"
            android:layout_alignTop="@+id/imageButton_torta1"
            android:layout_alignEnd="@+id/imageView_logo"
            android:layout_alignRight="@+id/imageView_logo"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="-33dp"
            android:layout_marginRight="-33dp"
            app:srcCompat="@mipmap/ic_launcher"
            tools:srcCompat="@drawable/morango" />

        <ImageButton
            android:id="@+id/imageButton_torta3"
            android:layout_width="118dp"
            android:layout_height="96dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="56dp"
            android:layout_marginLeft="56dp"
            android:layout_marginBottom="113dp"
            app:srcCompat="@drawable/ninhotrufado" />

        <ImageButton
            android:id="@+id/imageButton_torta4"
            android:layout_width="119dp"
            android:layout_height="96dp"
            android:layout_alignStart="@+id/imageButton_torta2"
            android:layout_alignLeft="@+id/imageButton_torta2"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-1dp"
            android:layout_marginLeft="-1dp"
            android:layout_marginBottom="114dp"
            app:srcCompat="@drawable/cheesecakeoreo" />

        <TextView
            android:id="@+id/textView_torta_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="97dp"
            android:layout_marginLeft="97dp"
            android:layout_marginBottom="222dp"
            android:text="@string/texto_torta_1" />

        <TextView
            android:id="@+id/textView_torta_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageButton_torta1"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="-242dp"
            android:layout_marginEnd="73dp"
            android:layout_marginRight="73dp"
            android:text="@string/texto_torta_2" />

        <TextView
            android:id="@+id/textView_torta_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="85dp"
            android:text="@string/texto_torta_3" />

        <TextView
            android:id="@+id/textView_torta_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_torta_3"
            android:layout_marginBottom="-28dp"
            android:text="@string/texto_torta_4" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>